{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": "aws-marketplace-management:*",
            "Resource": "*",
            "Condition": {
                "ForAnyValue:StringEquals": {
                    "aws:RequestedRegion": "US East (N.Virginia) us-east-1"
                }
            }
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": [
                "aws-marketplace:*",
                "s3:*",
                "ec2:*",
                "cloudformation:*"
            ],
            "Resource": "*"
        }
    ]
}
